{
  "name": "redux-boilerplate-helpers",
  "version": "0.0.3",
  "main": "./dist/index.js",
  "bin": {
    "rdxh": "./bin/index.js"
  },
  "license": "MIT",
  "repository": "https://github.com/alxlu/redux-boilerplate-helpers.git",
  "dependencies": {
    "assert": "^1.4.1",
    "babylon": "^6.17.2",
    "change-case": "^3.0.1",
    "invariant": "^2.2.2",
    "minimist": "^1.2.0",
    "prettier": "^1.4.4",
    "recast": "^0.13.1"
  },
  "scripts": {
    "start": "bin/index.js",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "build": "webpack",
    "format": "eslint src --fix && prettier --write --single-quote --trailing-comma=all --print-width=100 \"src/**/*.js\"",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "coveralls": "^2.13.1",
    "cross-spawn": "^5.1.0",
    "del": "^3.0.0",
    "eslint": "^4.1.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.48.0",
    "jest": "^20.0.4",
    "mkdirp": "^0.5.1",
    "webpack": "^3.0.0",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "coverageDirectory": "coverage",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "__*",
      "<rootDir>/src/cli.js",
      "<rootDir>/src/testUtils.js"
    ]
  }
}
